{
  "http": [
    {
      "connectionType": "sink",
      "eipId": {
        "namespace": "http",
        "name": "outbound-channel-adapter"
      },
      "attributes": [
        {
          "name": "url",
          "type": "string",
          "description": "URL to which the requests should be sent.",
          "required": false
        },
        {
          "name": "id",
          "type": "string",
          "required": false
        },
        {
          "name": "auto-startup",
          "type": "boolean",
          "description": "Boolean value indicating whether this endpoint should start automatically.",
          "required": false
        }
      ],
      "childGroup": {
        "indicator": "choice",
        "children": [
          {
            "occurrence": {
              "min": 0
            },
            "eipId": {
              "namespace": "integration",
              "name": "poller"
            },
            "attributes": [
              {
                "name": "fixed-rate",
                "type": "number",
                "description": "Fixed rate trigger.",
                "required": false
              },
              {
                "name": "time-unit",
                "type": "string",
                "required": false,
                "restriction": {
                  "type": "unknown",
                  "values": [
                    "MILLISECONDS",
                    "SECONDS"
                  ]
                }
              }
            ],
            "childGroup": {
              "indicator": "sequence",
              "children": [
                {
                  "occurrence": {
                    "min": 0,
                    "max": -1
                  },
                  "eipId": {
                    "namespace": "integration",
                    "name": "transactional"
                  },
                  "description": "Run each poll task in a transaction.",
                  "attributes": [
                    {
                      "name": "read-only",
                      "type": "string",
                      "description": "Is this transaction read-only?",
                      "default": "false",
                      "required": false
                    }
                  ]
                }
              ]
            }
          }
        ]
      }
    }
  ],
  "file": [
    {
      "role": "endpoint",
      "connectionType": "passthru",
      "attributes": [
        {
          "name": "input-channel",
          "type": "string",
          "description": "The input channel of the transformer.",
          "required": false
        },
        {
          "name": "output-channel",
          "type": "string",
          "description": "The channel to which the transformer will send the transformed message.",
          "required": false
        }
      ]
    }
  ]
}